{
  "port": 2002,
  "listen": "0.0.0.0",
  "protocol": "vless",
  "settings": {
    "clients": [
      {
        "id": ""
      }
    ],
    "decryption": "none"
  },
  "streamSettings": {
    "network": "gun",
    "security": "tls",
    "tlsSettings": {
      "serverName": "domain",
      "alpn": [
        "h2"
      ],
      "certificates": [
        {
          "certificateFile": "/usr/local/etc/v2ray/ssl/v2ray_ssl.crt",
          "keyFile": "/usr/local/etc/v2ray/ssl/v2ray_ssl.key"
        }
      ]
    },
    "grpcSettings": {
      "serviceName": "service_name"
    }
  }
}

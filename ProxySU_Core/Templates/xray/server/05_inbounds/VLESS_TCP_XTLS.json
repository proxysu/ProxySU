{
  "port": 443,
  "protocol": "vless",
  "settings": {
    "clients": [
      {
        "id": "",
        "flow": "xtls-rprx-direct"
      }
    ],
    "decryption": "none",
    "fallbacks": []
  },
  "streamSettings": {
    "network": "tcp",
    "security": "xtls",
    "xtlsSettings": {
      "alpn": [ "h2", "http/1.1" ],
      "certificates": [
        {
          "certificateFile": "/usr/local/etc/xray/ssl/xray_ssl.crt",
          "keyFile": "/usr/local/etc/xray/ssl/xray_ssl.key"
        }
      ]
    }
  }
}